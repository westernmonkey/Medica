 <math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mi>P</mi><mi>E</mi></math> is <math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mn>2</mn><mi>n</mi></math>.</p>", "image": null}, "videoSolution": {"videoId": "ea743463db2740019eec9ef31a1437a4", "provider": "vdocipher", "thumbnailLight": "https://d1z78r8i505acl.cloudfront.net/poster/dKGcrJomNlexa.480.jpeg", "thumbnailDark": "https://d1z78r8i505acl.cloudfront.net/poster/dKGcrJomNlexa.480.jpeg", "premiumModules": [{"premiumModuleId": "6628114842d540dfcb148ecb", "isFree": false}, {"premiumModuleId": "6788cc0b2dbc1ecacabb7026", "isFree": false}], "description": "NEET"}, "masterId": "MOGNEPYQ04811933", "numericalLowerLimit": null, "numericalUpperLimit": null, "bucket": "", "isRemoved": false, "cpyqbSubjects": ["61bb58181213731910ea0b70", "61c055eae8ce0dc8a5c62d33", "616057750283de43c87e3e1b", "61964fe0a5a5260118868021", "61964f81a5a526011886801d", "629f39e4ac11b26562e0625b", "616057920283de43c87e3e1e", "615f0c729476412f48314dab", "61a8a2c2e8ce0dc8a5c62d2d", "629105adee9e3ae00d63352b", "629f385f263330a77be9fa7d", "616056cd0283de43c87e3e15", "616057190283de43c87e3e18", "62962075e7f8e9ac4daeafd7", "64d50bd73df7f4a7f3017f6f", "66b79b34d4b564dfab4f94cc", "678ea9a09cc79e5f592bdc11", "67fa5fef4468eb1307ceb0ad", "6807d2ad647479b8e8696479"], "cpyqbChapters": ["616023f6105ab3ae165341bb", "61964c8546e3d33b1892d09e", "61c089968734366dae3f36bc", "61bb8217aebeece6abdbe72b", "616023f783ceaeb51fe46126", "616023f4946fab7b921bf28d", "61964c86b98428bab9bd89d6", "616023f51f5c75c27ba35382", "616023f36324da677f8a7444", "61e7bd75347c64f272d9d8bb", "62962124ca45d71899e3dfe9", "629f390dd4f42c0deef34eab", "629109ef78779344f598426c", "629f3a921cf4dc490b09a322", "64eb06c13118f713ecfef085", "66d5853ca75f05ee8564f57b", "67921d681d5b5072da5e9acb", "67fdff4e378e1624ea7b69f4", "6809202b3a10446a207fdee4"], "cpyqbExams": ["616059d70283de43c87e3e24", "616059730283de43c87e3e22"], "needToPurchaseModules": ["6628114842d540dfcb148ecb", "6788cc0b2dbc1ecacab