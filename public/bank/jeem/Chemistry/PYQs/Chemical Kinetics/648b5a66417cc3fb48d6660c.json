/mfrac><mo>=</mo><mo>-</mo><mn>5</mn><msup><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>2</mn></mrow></msup></math>  <br/><math><mrow><msubsup><mo stretchy=\"true\">\u222b</mo><mrow><mi mathvariant=\"normal\">e</mi><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub></mrow><mrow><mi mathvariant=\"normal\">N</mi></mrow></msubsup><mrow><msup><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mo>-</mo><mn>2</mn></mrow></msup><mi mathvariant=\"normal\">d</mi><mi mathvariant=\"normal\">N</mi></mrow></mrow><mo>=</mo><mo>-</mo><mn>5</mn><mrow><msubsup><mo stretchy=\"true\">\u222b</mo><mrow><mn>1</mn></mrow><mrow><mi mathvariant=\"normal\">t</mi></mrow></msubsup><mrow><mi mathvariant=\"normal\">d</mi><mi mathvariant=\"normal\">t</mi></mrow></mrow></math>  <br/><math><mfrac><mrow><mn>1</mn></mrow><mrow><mi mathvariant=\"normal\">N</mi></mrow></mfrac><mo>-</mo><mfrac><mrow><mn>1</mn></mrow><mrow><mi mathvariant=\"normal\">e</mi><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub></mrow></mfrac><mo>=</mo><mn>5</mn><mfenced separators=\"|\"><mrow><mi mathvariant=\"normal\">t</mi><mo>-</mo><mn>1</mn></mrow></mfenced></math>  <br/><math><mfrac><mrow><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub></mrow><mrow><mi mathvariant=\"normal\">N</mi></mrow></mfrac><mo>-</mo><mfrac><mrow><mn>1</mn></mrow><mrow><mi mathvariant=\"normal\">e</mi></mrow></mfrac><mo>=</mo><mn>5</mn><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub><mo>(</mo><mi mathvariant=\"normal\">t</mi><mo>-</mo><mn>1</mn><mo>)</mo></math>  <br/><math><mfrac><mrow><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub></mrow><mrow><mi mathvariant=\"normal\">N</mi></mrow></mfrac><mo>=</mo><mn>5</mn><msub><mrow><mi mathvariant=\"normal\">N</mi></mrow><mrow><mn>0</mn></mrow></msub><mo>(</mo><mi mathvariant=\"normal\">t</mi><mo>-</mo><mn>1</mn><mo>)</mo><mo>+</mo>